  "author": "ashigirl96 <reon@mma.cs.tsukuba.ac.jp>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev --respawn src/index.ts",
    "lint": "eslint --ext .js,.ts,.tsx ./src --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
